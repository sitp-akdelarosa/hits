        if err <> 0 then
          DisConnDBH ObjConn, ObjRS	'DB切断
          jampErrerP "1","b302","04","空搬出：データ登録","102","元請陸運業者名取得に失敗<BR>"&StrSQL
        end if
        FullName = "'" & ObjRS("FullName") & "'"
        ObjRS.close
