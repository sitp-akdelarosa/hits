                 "ContSize1='"& Request("ContSize0") &"',ContType1='"& Request("ContType0") &"',ContHeight1='"& Request("ContHeight0") &"',"&_
                 "ContMaterial1='"& Request("Material0") &"',PickPlace1='"& Request("PickPlace0") &"',Qty1="& Qty1 &", "&_
