  ElseIF Request("Mord") = 1 Then	'更新
    ret=true
    Dim oldCOMPcd1
    oldCOMPcd1=Request("oldCOMPcd1")
    If oldCOMPcd1 = "" Then
      oldCOMPcd1= " "
    End If
      If Trim(COMPcd1) <> "" Then
    '元請陸運業者名取得
        StrSQL = "SELECT FullName FROM mUsers WHERE mUsers.HeadCompanyCode='" & COMPcd1 &"'"
        ObjRS.Open StrSQL, ObjConn
        if err <> 0 then
